{
  "name": "edu.juejin.fun",
  "version": "0.0.1",
  "description": "掘金社区学习资料",
  "main": "index.js",
  "repository": "https://github.com/xitu/courses",
  "scripts": {
    "start": "npm run docz:dev",
    "docz:dev": "rm -rf .docz && docz dev --port=3000",
    "docz:build": "rm -rf .docz && docz build",
    "docz:serve": "rm -rf .docz && docz build && docz serve",
    "prepublishOnly": "npm run docz:build && rm -rf docs && cp -R .docz/dist docs",
    "deploy": "npm run prepublishOnly && http-server docs -c-1 -p8080",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@emotion/styled": "^10.3.0",
    "docz": "^2.4.0",
    "http-server": "^14.1.0",
    "katex": "^0.15.6",
    "mermaid": "^9.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rehype-katex": "^5.0.0",
    "remark-emoji": "^2.2.0",
    "remark-hint": "^1.0.10",
    "remark-math": "^3.0.1",
    "remark-mdx": "2.0.0-next.9"
  }
}
